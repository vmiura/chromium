import "ui/gfx/geometry/mojo/geometry.mojom";

enum UIResourceFormat {
  RGBA8,
  ALPHA_8,
  ETC1,
};

struct UIResourceBitmap {
  array<int8> bitmap;
};

enum UIResourceRequestType {
  CREATE_REQUEST,
  DELETE,
  INVALID,
};

struct UIResourceRequestProperties {
  int32 uid;
  UIResourceRequestType type;
  UIResourceBitmap? bitmap;
};
